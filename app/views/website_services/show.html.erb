<%- model_class = WebsiteService -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @website_service.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:description) %>:</strong></dt>
  <dd><%= @website_service.description %></dd>
  <dt><strong><%= model_class.human_attribute_name(:redirect_url) %>:</strong></dt>
  <dd><%= @website_service.redirect_url %></dd>
</dl>

<%= link_to('Edit', '/places/' + params[:place_id] + '/website_services/' + params[:id] + '/edit', :class => 'btn btn-default') %>

<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              website_service_path(@website_service),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
